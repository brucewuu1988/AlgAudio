<collection version="1" id="base">
  <name>Base modules</name>
  <defaultlib file="base.aa"/>
  <module id="pipe" name="Pipe">
    <parrams>
      <inlet id="inbus"/>
      <outlet id="outbus"/>
    </parrams>
    <sc>
arg inbus, outbus;
var in = In.ar(inbus);
Out.ar(outbus,in);
    </sc>
  </module>
  <module id="simpleout" name="Simple output">
    <parrams>
      <inlet id="inbus"/>
    </parrams>
    <sc>
arg inbus;
var in = In.ar(inbus);
Out.ar(0,in);
    </sc>
  </module>
  <module id="simplein" name="Simple input">
    <parrams>
      <inlet id="output"/>
    </parrams>
    <sc>
arg outbus;
var in = SoundIn.ar(0);
Out.ar(outbus,in);
    </sc>
  </module>
  <module id="stereoout" name="Stereo output">
    <parrams>
      <inlet id="inbus1"/>
      <inlet id="inbus2"/>
    </parrams>
    <sc>
arg inbus1, inbus2;
Out.ar(0,In.ar(inbus1));
Out.ar(1,In.ar(inbus2));
    </sc>
  </module>


  <module id="sine" name="Sine oscillator">
    <parrams>
      <outlet id="outbus"/>
    </parrams>
    <sc>
arg freq=440, amp=1, outbus;
Out.ar(outbus, SinOsc.ar(freq,0,amp));
    </sc>
  </module>

</collection>
