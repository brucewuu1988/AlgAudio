<collection version="1" id="debug">
  <name>Debug utilities</name>
  <defaultlib file="debug.aa"/>
  <module id="pipe" name="Pipe">
    <sc>
arg inbus, outbus;
var in = In.ar(inbus);
Out.ar(outbus,in);
    </sc>
  </module>
  <module id="helloworld" name="Hello World!">
    <class name="HelloWorld"/>
  </module>
  <module id="window" name="Custom window">
    <class name="Window"/>
  </module>
  <module id="console" name="SCLang console view">
    <class name="Console"/>
  </module>
  <module id="debugsrc" name="Debug sound source">
    <parrams>
      <inlet id="aaa"/>
    </parrams>
    <sc>
Out.ar(0,SinOsc.ar(500,0,1));
Out.ar([0,1], In.ar(10,1));
    </sc>
  </module>
  <module id="debugout" name="Debug sound output">
    <sc>
Out.ar(1,SinOsc.ar(440,0,1));
Out.ar(10,WhiteNoise.ar(0.5));
    </sc>
  </module>
</collection>
